# Browseek Configuration

# Maximum number of concurrent browser instances
MAX_CONCURRENT_BROWSERS=5

# Default timeout for browser tasks (in seconds)
DEFAULT_TIMEOUT=30

# Number of retry attempts for failed tasks
RETRY_ATTEMPTS=3

# Proxy configuration
PROXY_ENABLED=True
PROXY_ROTATE_ON_FAILURE=True

# DNS over HTTPS configuration
DNS_OVER_HTTPS=True

# CAPTCHA solving service configuration
CAPTCHA_SERVICE=2captcha
CAPTCHA_API_KEY=your_api_key_here

# Test configuration
TEST_URL=https://example.local
